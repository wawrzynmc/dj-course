{
  "name": "azor-chatdog",
  "version": "1.0.0",
  "description": "Azor the Chatdog - Interactive terminal-based chat application with multi-LLM support",
  "main": "dist/run.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/run.js",
    "dev": "tsx src/run.ts",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "check:types": "tsc --noEmit"
  },
  "keywords": [
    "chatbot",
    "ai",
    "gemini",
    "llama",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/genai": "^1.34.0",
    "chalk": "^5.6.2",
    "dotenv": "^17.2.3",
    "inquirer": "^13.1.0",
    "markdown-it": "^14.1.0",
    "node-llama-cpp": "^3.14.5",
    "openai": "^6.15.0",
    "pdf-lib": "^1.17.1",
    "uuid": "^13.0.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.9",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^25.0.3",
    "@types/uuid": "^11.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
